<h1>Import starships</h1>

<%= form_tag starship_import_path(@conn, :import_starships) do %>
  <div class="form-group">
    <label for="starship_ids">Starship ids</label>
    <input type="text" class="form-control" id="starhip_ids" name="starship_ids">
  </div>

  <button type="submit" class="btn btn-default">Submit</button>
<% end %>
