<h1>Starships imported</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Model</th>
      <th>Manufacturer</th>
    </tr>
  </thead>

  <tbody>
    <%= for starship <- @starships do %>
      <tr>
        <td><%= starship.name %></td>
        <td><%= starship.model %></td>
        <td><%= starship.manufacturer %></td>
      </tr>
    <% end %>
  </tbody>
</table>
